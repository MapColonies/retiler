{
  "name": "retiler",
  "version": "4.0.0",
  "description": "processor of metatiles into stored tiles",
  "main": "./src/index.ts",
  "scripts": {
    "test:unit": "jest --config=./tests/configurations/unit/jest.config.js",
    "test:integration": "jest --config=./tests/configurations/integration/jest.config.js",
    "format": "prettier --check .",
    "format:fix": "prettier --write .",
    "prelint:fix": "npm run format:fix",
    "prelint": "npm run format",
    "lint": "eslint .",
    "lint:fix": "eslint --fix .",
    "release": "standard-version",
    "test": "tsc --noEmit && npm run test:unit && npm run test:integration",
    "prebuild": "npm run clean",
    "build": "tsc --project tsconfig.build.json && tsc-alias -p tsconfig.build.json && npm run assets:copy",
    "start": "npm run build && cd dist && node --import ./instrumentation.mjs ./index.js",
    "start:dev": "npm run build && cd dist && cross-env CONFIG_OFFLINE_MODE=true node --enable-source-maps --import ./instrumentation.mjs ./index.js",
    "assets:copy": "copyfiles -f ./config/* ./dist/config && copyfiles -f ./dist/ && copyfiles ./package.json dist",
    "clean": "rimraf dist",
    "prepare": "node .husky/install.mjs"
  },
  "directories": {
    "test": "tests"
  },
  "author": "MapColonies",
  "license": "ISC",
  "dependencies": {
    "@aws-sdk/client-s3": "^3.850.0",
    "@godaddy/terminus": "^4.12.1",
    "@map-colonies/cleanup-registry": "^1.1.0",
    "@map-colonies/config": "^3.0.0",
    "@map-colonies/detiler-client": "^1.3.0",
    "@map-colonies/detiler-common": "^1.3.0",
    "@map-colonies/error-express-handler": "^3.0.0",
    "@map-colonies/express-access-log-middleware": "^3.0.1",
    "@map-colonies/js-logger": "^2.0.0",
    "@map-colonies/read-pkg": "^1.0.0",
    "@map-colonies/schemas": "^1.9.0",
    "@map-colonies/telemetry": "^10.0.1",
    "@map-colonies/tile-calc": "^0.1.5",
    "@opentelemetry/api": "^1.9.0",
    "ajv": "^8.17.1",
    "axios": "^1.10.0",
    "compression": "^1.8.1",
    "express": "^4.21.2",
    "http-status-codes": "^2.3.0",
    "pg-boss": "^7.1.0",
    "prom-client": "^15.1.3",
    "reflect-metadata": "^0.2.2",
    "serialize-error": "^8.1.0",
    "sharp": "^0.34.3",
    "string-format": "^2.0.0",
    "tsyringe": "^4.10.0"
  },
  "devDependencies": {
    "@commitlint/cli": "^19.8.1",
    "@commitlint/config-conventional": "^19.8.1",
    "@faker-js/faker": "^9.9.0",
    "@jest/globals": "^30.0.5",
    "@map-colonies/eslint-config": "^6.0.0",
    "@map-colonies/prettier-config": "0.0.1",
    "@map-colonies/tsconfig": "^1.0.1",
    "@smithy/types": "^4.3.1",
    "@swc/core": "^1.11.11",
    "@swc/jest": "^0.2.37",
    "@types/compression": "^1.8.1",
    "@types/express": "^4.17.21",
    "@types/jest": "^30.0.0",
    "@types/js-yaml": "^4.0.9",
    "@types/multer": "^1.4.12",
    "@types/string-format": "^2.0.3",
    "@types/supertest": "^6.0.2",
    "copyfiles": "^2.4.1",
    "eslint": "^9.23.0",
    "eslint-plugin-jest": "^28.11.0",
    "husky": "^9.1.7",
    "jest": "^30.0.5",
    "jest-html-reporters": "^3.1.7",
    "jest-create-mock-instance": "^2.0.0",
    "js-yaml": "^4.1.0",
    "lodash": "^4.17.21",
    "nock": "^13.5.6",
    "prettier": "^3.5.3",
    "pretty-quick": "^4.1.1",
    "rimraf": "^6.0.1",
    "supertest": "^7.1.0",
    "ts-jest": "^29.2.6",
    "tsc-alias": "^1.8.11",
    "typescript": "^5.8.2"
  }
}
