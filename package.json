{
  "name": "retiler",
  "version": "3.0.0",
  "description": "tiles and stores a web map",
  "main": "./src/index.ts",
  "scripts": {
    "test:unit": "jest --config=./tests/configurations/unit/jest.config.js",
    "test:integration": "jest --config=./tests/configurations/integration/jest.config.js",
    "format": "prettier --check .",
    "format:fix": "prettier --write .",
    "prelint:fix": "npm run format:fix",
    "prelint": "npm run format",
    "lint": "eslint .",
    "lint:fix": "eslint --fix .",
    "release": "standard-version",
    "test": "npm run test:unit && npm run test:integration",
    "prebuild": "npm run clean",
    "build": "tsc --project tsconfig.build.json && npm run assets:copy",
    "start": "npm run build && cd dist && node ./index.js",
    "assets:copy": "copyfiles -f ./config/* ./dist/config && copyfiles ./package.json dist",
    "clean": "rimraf dist",
    "install": "npx husky install"
  },
  "directories": {
    "test": "tests"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/MapColonies/retiler.git"
  },
  "author": "MapColonies",
  "license": "ISC",
  "husky": {
    "hooks": {
      "pre-commit": "pretty-quick --staged",
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "dependencies": {
    "@aws-sdk/client-s3": "^3.53.1",
    "@godaddy/terminus": "4.9.0",
    "@map-colonies/cleanup-registry": "^1.1.0",
    "@map-colonies/detiler-client": "^1.0.0-rc3",
    "@map-colonies/detiler-common": "^1.0.0-rc4",
    "@map-colonies/error-express-handler": "^2.1.0",
    "@map-colonies/express-access-log-middleware": "^1.0.0",
    "@map-colonies/js-logger": "^0.0.5",
    "@map-colonies/read-pkg": "0.0.1",
    "@map-colonies/telemetry": "4.2.0",
    "@map-colonies/tile-calc": "^0.1.5",
    "@opentelemetry/api": "1.1.0",
    "@opentelemetry/api-metrics": "0.29.0",
    "ajv": "^8.12.0",
    "axios": "^0.26.1",
    "compression": "^1.7.4",
    "config": "^3.3.6",
    "express": "^4.18.2",
    "http-status-codes": "^2.1.4",
    "pg-boss": "^7.1.0",
    "prom-client": "^14.2.0",
    "reflect-metadata": "^0.1.13",
    "serialize-error": "^8.1.0",
    "sharp": "^0.32.0",
    "string-format": "^2.0.0",
    "tsyringe": "^4.6.0"
  },
  "devDependencies": {
    "@commitlint/cli": "^13.2.1",
    "@commitlint/config-conventional": "^13.2.0",
    "@faker-js/faker": "^6.1.1",
    "@map-colonies/eslint-config": "^3.1.1",
    "@map-colonies/prettier-config": "0.0.1",
    "@map-colonies/standard-version-update-helm-version": "^2.0.1",
    "@smithy/types": "^3.3.0",
    "@types/compression": "^1.7.2",
    "@types/config": "0.0.39",
    "@types/express": "^4.17.13",
    "@types/jest": "^29.5.11",
    "@types/js-yaml": "^4.0.3",
    "@types/multer": "^1.4.7",
    "@types/node": "^17.0.21",
    "@types/string-format": "^2.0.0",
    "@types/supertest": "^2.0.11",
    "commitlint": "^13.2.1",
    "copyfiles": "^2.4.1",
    "cz-conventional-changelog": "^3.3.0",
    "eslint": "^8.15.0",
    "husky": "^7.0.2",
    "jest": "^29.7.0",
    "jest-create-mock-instance": "^2.0.0",
    "jest-html-reporters": "^3.1.7",
    "js-yaml": "^4.1.0",
    "lodash": "^4.17.21",
    "nock": "^13.2.4",
    "prettier": "^2.4.1",
    "pretty-quick": "^3.1.1",
    "rimraf": "^3.0.2",
    "standard-version": "^9.3.2",
    "supertest": "^6.1.6",
    "ts-jest": "^29.1.1",
    "typescript": "^4.6.3"
  }
}
