{
  "name": "retiler",
  "version": "3.2.0",
  "description": "tiles and stores a web map",
  "main": "./src/index.ts",
  "scripts": {
    "test:unit": "jest --config=./tests/configurations/unit/jest.config.js",
    "test:integration": "jest --config=./tests/configurations/integration/jest.config.js",
    "format": "prettier --check .",
    "format:fix": "prettier --write .",
    "prelint:fix": "npm run format:fix",
    "prelint": "npm run format",
    "lint": "eslint .",
    "lint:fix": "eslint --fix .",
    "test": "tsc --noEmit -p tsconfig.test.json && npm run test:unit && npm run test:integration",
    "prebuild": "npm run clean",
    "build": "tsc --project tsconfig.build.json && tsc-alias -p tsconfig.build.json && npm run assets:copy",
    "start": "npm run build && cd dist && node --import ./instrumentation.mjs ./index.js",
    "start:dev": "npm run build && cd dist && cross-env ALLOW_CONFIG_MUTATIONS=true CONFIG_OFFLINE_MODE=true  node --enable-source-maps --import ./instrumentation.mjs ./index.js",
    "assets:copy": "copyfiles -f ./config/* ./dist/config && copyfiles ./package.json dist",
    "clean": "rimraf dist",
    "prepare": "node .husky/install.mjs"
  },
  "directories": {
    "test": "tests"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/MapColonies/retiler.git"
  },
  "author": "MapColonies",
  "license": "ISC",
  "dependencies": {
    "@aws-sdk/client-s3": "^3.740.0",
    "@common.js/serialize-error": "^11.0.3",
    "@godaddy/terminus": "4.12.1",
    "@map-colonies/cleanup-registry": "^1.1.0",
    "@map-colonies/config": "^2.1.0",
    "@map-colonies/detiler-client": "^1.3.0",
    "@map-colonies/detiler-common": "^1.3.0",
    "@map-colonies/error-express-handler": "^2.1.0",
    "@map-colonies/express-access-log-middleware": "^2.0.1",
    "@map-colonies/js-logger": "^1.0.1",
    "@map-colonies/read-pkg": "0.0.1",
    "@map-colonies/schemas": "https://ghatmpstorage.blob.core.windows.net/npm-packages/schemas-6fda0a73fbe410e617c1524eabf17c4511350399.tgz",
    "@map-colonies/telemetry": "^7.0.1",
    "@map-colonies/tile-calc": "^0.1.5",
    "@opentelemetry/api": "1.7.0",
    "ajv": "^8.17.1",
    "axios": "^1.7.9",
    "compression": "^1.7.4",
    "express": "^4.19.2",
    "http-status-codes": "^2.1.4",
    "pg-boss": "^7.1.0",
    "prom-client": "^15.1.2",
    "reflect-metadata": "^0.1.13",
    "sharp": "^0.32.0",
    "string-format": "^2.0.0",
    "tsyringe": "^4.8.0"
  },
  "devDependencies": {
    "@commitlint/cli": "^19.5.0",
    "@commitlint/config-conventional": "^19.5.0",
    "@faker-js/faker": "^9.4.0",
    "@map-colonies/eslint-config": "^4.0.0",
    "@map-colonies/prettier-config": "0.0.1",
    "@smithy/types": "^3.3.0",
    "@swc/core": "^1.7.26",
    "@swc/jest": "^0.2.36",
    "@types/compression": "^1.7.2",
    "@types/express": "^5.0.0",
    "@types/jest": "^29.5.14",
    "@types/js-yaml": "^4.0.9",
    "@types/node": "^22.13.0",
    "@types/string-format": "^2.0.3",
    "@types/supertest": "^6.0.2",
    "copyfiles": "^2.4.1",
    "cross-env": "^7.0.3",
    "eslint": "^8.43.0",
    "husky": "^9.1.6",
    "jest": "^29.7.0",
    "jest-create-mock-instance": "^2.0.0",
    "jest-html-reporters": "^3.1.7",
    "js-yaml": "^4.1.0",
    "lodash": "^4.17.21",
    "nock": "^13.2.4",
    "prettier": "^3.3.3",
    "pretty-quick": "^4.0.0",
    "rimraf": "^5.0.1",
    "supertest": "^7.0.0",
    "ts-jest": "^29.2.5",
    "tsc-alias": "^1.8.10",
    "typescript": "^5.1.3"
  }
}
