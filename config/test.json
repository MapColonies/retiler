{
  "app": {
    "parallelism": 2,
    "queueName": "TEST_QUEUE",
    "jobQueue": {
      "waitTimeout": 50,
      "pgBoss": {
        "host": "postgres",
        "port": 5432,
        "user": "postgres",
        "password": "1234",
        "schema": "public",
        "enableSslAuth": false,
        "database": "postgres",
        "noSupervisor": true
      }
    },
    "tilesStorage": {
      "providers": [
        {
          "type": "s3",
          "endpoint": "http://minio1:9003",
          "bucketName": "test1",
          "region": "us-east-1",
          "forcePathStyle": true,
          "credentials": {
            "accessKeyId": "minioadmin1",
            "secretAccessKey": "minioadmin1"
          }
        },
        {
          "type": "s3",
          "endpoint": "http://minio2:9004",
          "bucketName": "test2",
          "region": "us-east-1",
          "forcePathStyle": true,
          "credentials": {
            "accessKeyId": "minioadmin2",
            "secretAccessKey": "minioadmin2"
          }
        },
        {
          "type": "fs",
          "basePath": "/tmp/test"
        }
      ],
      "layout": {
        "format": "prefix/{z}/{x}/{y}.png",
        "shouldFlipY": true
      }
    }
  }
}
