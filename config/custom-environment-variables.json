{
  "telemetry": {
    "serviceName": "TELEMETRY_SERVICE_NAME",
    "hostname": "TELEMETRY_HOST_NAME",
    "version": "TELEMETRY_SERVICE_VERSION",
    "logger": {
      "level": "LOG_LEVEL",
      "prettyPrint": {
        "__name": "LOG_PRETTY_PRINT_ENABLED",
        "__format": "boolean"
      }
    },
    "tracing": {
      "enabled": "TELEMETRY_TRACING_ENABLED",
      "url": "TELEMETRY_TRACING_URL"
    },
    "metrics": {
      "enabled": "TELEMETRY_METRICS_ENABLED",
      "url": "TELEMETRY_METRICS_URL",
      "interval": "TELEMETRY_METRICS_INTERVAL"
    }
  },
  "server": {
    "port": "SERVER_PORT"
  },
  "app": {
    "projectName": "APP_PROJECT_NAME",
    "queueName": "QUEUE_NAME",
    "jobQueue": {
      "pgBoss": {
        "host": "DB_HOST",
        "port": {
          "__name": "DB_PORT",
          "__format": "number"
        },
        "user": "DB_USERNAME",
        "password": "DB_PASSWORD",
        "schema": "DB_SCHEMA",
        "enableSslAuth": {
          "__name": "DB_ENABLE_SSL_AUTH",
          "__format": "boolean"
        },
        "sslPaths": {
          "ca": "DB_CA_PATH",
          "key": "DB_KEY_PATH",
          "cert": "DB_CERT_PATH"
        },
        "database": "DB_NAME",
        "noSupervisor": {
          "__name": "NO_SUPERVISOR",
          "__format": "boolean"
        }
      }
    },
    "map": {
      "url": "MAP_URL",
      "format": "MAP_FORMAT",
      "client": {
        "timeoutMs": {
          "__name": "MAP_CLIENT_TIMEOUT_MS",
          "__format": "number"
        }
      }
    },
    "tilesStorage": {
      "s3ClientConfig": {
        "endpoint": "S3_ENDPOINT",
        "region": "S3_REGION",
        "forcePathStyle": {
          "__name": "S3_FORCE_PATH_STYLE",
          "__format": "boolean"
        }
      },
      "s3Bucket": "S3_BUCKET",
      "layout": {
        "format": "TILES_STORAGE_LAYOUT_FORMAT",
        "shouldFlipY": {
          "__name": "TILES_STORAGE_LAYOUT_SHOULD_FLIP_Y",
          "__format": "boolean"
        }
      }
    }
  }
}
