{
  "telemetry": {
    "logger": {
      "level": "info",
      "prettyPrint": false
    }
  },
  "server": {
    "port": "8080"
  },
  "app": {
    "projectName": "default",
    "queueName": "QUEUE_NAME",
    "parallelism": 5,
    "jobQueue": {
      "pgBoss": {
        "host": "localhost",
        "port": 5432,
        "user": "postgres",
        "password": "postgres",
        "schema": "pgboss",
        "enableSslAuth": false,
        "sslPaths": {
          "ca": "",
          "key": "",
          "cert": ""
        },
        "database": "pgboss",
        "noSupervisor": true
      }
    },
    "map": {
      "url": "https://some-domain.com/api/",
      "format": "png32",
      "client": {
        "timeoutMs": 60000
      }
    },
    "tilesStorage": {
      "s3ClientConfig": {
        "endpoint": "http://s3-domain/",
        "region": "region",
        "forcePathStyle": true
      },
      "s3Bucket": "bucket-name",
      "layout": {
        "format": "prefix/{z}/{x}/{y}.png",
        "shouldFlipY": true
      }
    }
  }
}
