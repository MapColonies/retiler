{
  "openapiConfig": {},
  "telemetry": {
    "logger": {
      "level": "info",
      "prettyPrint": false
    },
    "metrics": {
      "enabled": true,
      "buckets": [0.1, 0.5, 1, 5, 15, 50, 250, 500]
    },
    "tracing": {
      "isEnabled": false
    },
    "shared": {}
  },
  "server": {
    "port": 8080
  },
  "app": {
    "project": {
      "name": "default",
      "stateUrl": "defaultStateUrl"
    },
    "forceProcess": false,
    "queueName": "tiles-default",
    "parallelism": 5,
    "jobQueue": {
      "waitTimeout": 5000,
      "pgBoss": {
        "host": "localhost",
        "port": 5432,
        "username": "postgres",
        "password": "postgres",
        "schema": "pgboss",
        "ssl": {
          "enabled": false,
          "ca": "",
          "key": "",
          "cert": ""
        },
        "database": "postgres",
        "noSupervisor": true
      }
    },
    "map": {
      "provider": "wms",
      "url": "https://vector-qgis-server-np.apps.j1lk3njp.eastus.aroapp.io/qgis-server?MAP=data/gush/gush.qgs",
      "format": "image/png",
      "wms": {
        "version": "1.3.0",
        "layers": "SUB_GUSH_ALL",
        "isZoomLayers": false,
        "styles": ""
      },
      "client": {
        "timeoutMs": 60000
      }
    },
    "tilesStorage": {
      "providers": [
        {
          "kind": "s3",
          "endpoint": "http://localhost:9000/",
          "bucketName": "test1",
          "region": "region",
          "forcePathStyle": true,
          "credentials": {
            "accessKeyId": "minio",
            "secretAccessKey": "minio123"
          }
        }
      ],
      "layout": {
        "format": "test/{z}/{x}/{y}.png",
        "shouldFlipY": true
      },
      "shouldFilterBlankTiles": false
    }
  },
  "detiler": {
    "enabled": false,
    "proceedOnFailure": true
  }
}
