{
  "telemetry": {
    "logger": {
      "level": "info",
      "prettyPrint": false
    },
    "metrics": {
      "buckets": [0.1, 0.5, 1, 5, 15, 50, 250, 500],
      "port": 8083
    }
  },
  "server": {
    "port": "8080"
  },
  "app": {
    "projectName": "default",
    "queueName": "QUEUE_NAME",
    "parallelism": 5,
    "jobQueue": {
      "waitTimeout": 5000,
      "pgBoss": {
        "host": "localhost",
        "port": 5432,
        "user": "postgres",
        "password": "postgres",
        "schema": "pgboss",
        "enableSslAuth": false,
        "sslPaths": {
          "ca": "",
          "key": "",
          "cert": ""
        },
        "database": "pgboss",
        "noSupervisor": true
      }
    },
    "map": {
      "provider": "arcgis",
      "url": "https://some-domain.com/api/",
      "format": "png32",
      "wms": {
        "version": "1.3.0"
      },
      "client": {
        "timeoutMs": 60000
      }
    },
    "tilesStorage": {
      "s3ClientConfig": {
        "endpoint": "http://s3-domain/",
        "region": "region",
        "forcePathStyle": true
      },
      "s3Bucket": "bucket-name",
      "layout": {
        "format": "prefix/{z}/{x}/{y}.png",
        "shouldFlipY": true
      }
    }
  }
}
