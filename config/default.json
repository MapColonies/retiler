{
  "telemetry": {
    "logger": {
      "level": "info",
      "prettyPrint": false
    },
    "metrics": {
      "enabled": true,
      "buckets": [
        0.1,
        0.5,
        1,
        5,
        15,
        50,
        250,
        500
      ]
    }
  },
  "server": {
    "port": "8080"
  },
  "app": {
    "project": {
      "name": "default",
      "stateUrl": "defaultStateUrl"
    },
    "forceProcess": false,
    "queueName": "QUEUE_NAME",
    "parallelism": 5,
    "jobQueue": {
      "waitTimeout": 5000,
      "pgBoss": {
        "host": "localhost",
        "port": 5432,
        "user": "postgres",
        "password": "postgres",
        "schema": "pgboss",
        "enableSslAuth": false,
        "sslPaths": {
          "ca": "",
          "key": "",
          "cert": ""
        },
        "database": "pgboss",
        "noSupervisor": true
      }
    },
    "map": {
      "provider": "arcgis",
      "url": "https://some-domain.com/api/",
      "format": "png32",
      "wms": {
        "version": "1.3.0"
      },
      "client": {
        "timeoutMs": 60000
      }
    },
    "tilesStorage": {
      "providers": [
        {
          "kind": "s3",
          "endpoint": "http://s3-domain/",
          "bucketName": "bucket-name",
          "region": "region",
          "forcePathStyle": true,
          "credentials": {
            "accessKeyId": "accessKeyId",
            "secretAccessKey": "secret"
          }
        }
      ],
      "layout": {
        "format": "prefix/{z}/{x}/{y}.png",
        "shouldFlipY": true
      },
      "shouldFilterBlankTiles": false
    }
  },
  "detiler": {
    "enabled": false
  }
}
